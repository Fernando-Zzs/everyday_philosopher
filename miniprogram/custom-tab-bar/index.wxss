
.tab-bar {
  position: fixed;
  /* bottom: 0; */
  left: 0;
  right: 0;
  height: 50px;
  background:rgb(39, 39, 39);
  display: flex;
  backdrop-filter: blur(20px);

  /* border: solid 2px red; */
  /* padding-bottom: env(safe-area-inset-bottom);  */
}

.tab-bar-border {
  /* background-color: rgb(196, 60, 60); */
  position: absolute;
  left: 0;
  top: 0;

  width: 100%;
  height: 2px;
}
.chose{
  position: absolute;
  background:linear-gradient(45deg,#052be0 0%,#8200f4 100%);
  border-radius: 24px;
  width: 33.3333%;
  height: 100%;
  z-index: -1;
  transition: All 1s cubic-bezier(0.68,-0.1,0.26,1); 
  /* transform:translateY(50px); */
}


.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.tab-bar-item cover-image {
  width: 27px;
  height: 27px;
}

.tab-bar-item cover-view {
  font-size: 10px;
} 

.tab-text{
  margin-top: 5px;
  /* padding: 5px; */
}