.page {  
  margin-left: 0rpx;  
  margin-right: 0rpx;  
}

.swiper-tab {
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  line-height: 60rpx;
}

.tab-item {
  width: 50%;
  text-align: center;
  font-size: 28rpx;
  color: #333;
  background-color: #D0A669;
}

.swiper {
  height: 5000rpx;
  background: #dfdfdf;
}

.on {
  color: #fff;
  border-bottom: 10rpx solid #fff;
}

.zero,
.one{
  position: absolute;
  top:0;
  bottom: 0;
  left: 0;
  right: 0;

  display:flex;
  flex-direction: column;
  box-sizing: border-box;
}

.mychart-dom-line{
  z-index: 999;
  width: 100%;
  height: 200rpx;
}