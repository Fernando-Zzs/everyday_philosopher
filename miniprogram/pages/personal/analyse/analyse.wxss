.circle-item {
  position: absolute;
  width: 300rpx;
  height: 300rpx;
  border-radius: 50%;
  animation: float 2.5s infinite ease-in-out;
  backdrop-filter: blur(5px);
  border: white 1px solid;
  background: rgba(255, 255, 255, 0.349);
  text-align: center;
  line-height: 100px;
}

@keyframes float {
  0% {
    transform: translate(0px, 0px) scale(1);
  }

  50% {
    transform: translate(0px, 10px) scale(1.1);
  }

  100% {
    transform: translate(0px, 0px) scale(1);
  }
}

